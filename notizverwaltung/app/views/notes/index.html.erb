<h1>Notes#index</h1>
<p>Find me in app/views/notes/index.html.erb</p>

<table>
	<tr>
    	<th>id</th>
    	<th>Notiz</th>
  	</tr>
	<% @notes.to_a.each do |note| %>
 		<%= render partial: "note", locals: {note: note} %>
 	<% end %>
		
</table>
<%= button_to "Neue Notiz", new_note_path, :class => "inbox-sf-add-btn tip", :method => :get %>